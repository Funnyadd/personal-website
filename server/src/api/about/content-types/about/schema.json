    },
    "description": {
      "type": "text"
    },
    "educations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::education.education"
    },
    "experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience"
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill"
    },
    "languages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::language.language"
    },
    "about_counters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::about-counter.about-counter"
    },
    "navigation": {
      "type": "json"
    },
    "Photo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
